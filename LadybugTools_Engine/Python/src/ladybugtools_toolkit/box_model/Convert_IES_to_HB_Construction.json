    HB_material["conductivity"] = layer.material_properties.conductivity
    HB_material["specific_heat"] = layer.material_properties.specific_heat_capacity
    HB_material["density"] = layer.material_properties.density
    HB_material["thermal_absorptance"] = layer.material_properties.conductivity


